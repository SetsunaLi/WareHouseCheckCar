            android:text="空架" />

        <View style="@style/view_item_mid" />

        <TextView
            style="@style/text_item"
            android:layout_width="100dip"
            android:layout_height="match_parent"
            android:text="客户PO" />

    </LinearLayout>

    <View style="@style/view_item_bottom" />

